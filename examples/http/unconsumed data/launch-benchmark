#! /bin/bash
DIR=../../..
gnome-terminal -- /bin/bash -c "swipl -p node=$DIR/prolog $DIR/prolog/server.pl  --  $DIR/examples/http/unconsumed\ data/unconsumed.pl"
for i in `seq 1 10`
do 
sleep 5
node server.js &
sleep 5
### client-side instrumentation
node $DIR/node_modules/jalangi2/src/js/commands/jalangi.js --inlineIID --inlineSource --analysis $DIR/jalangi/functionInvocationAnalysis.js transpiled/benchmark-client.js $1 | grep -e 'Requests.*$' >> results-monitored.txt 
pkill node
sleep 5
done
pkill swipl

