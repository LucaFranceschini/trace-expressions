#! /bin/bash
DIR=../../..
gnome-terminal -- /bin/bash -c "swipl -p node=$DIR/prolog $DIR/prolog/server.pl  --  $DIR/examples/http/omitted\ body/204\ response/spec.pl"
for i in `seq 1 10`
do 
sleep 5
node $DIR/examples/express-examples/instrumentation/node_modules/jalangi2/src/js/commands/direct.js --analysis $DIR/jalangi/functionInvocationAnalysis.js transpiled/simple_server-ecma5.js &
sleep 5
node benchmark-client.js $1 >> results-monitored.txt
pkill node
sleep 5
done
pkill swipl
